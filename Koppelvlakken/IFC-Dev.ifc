CONTEXT DeveloperInterfacting
-----------------------------------------------------------
INTERFACE "[Overview]" FOR Developer  : I[ONE]
BOX[ "Communities Of Interest (COIs)" : V[ONE*CommunityOfInterest]
   , "InformationDomain (IDs)"        : V[ONE*InformationDomain]
   , "InterconnectionPanes (ICPs)"    : V[ONE*InterconnectionPane]
   , "SPEPs"                          : V[ONE*SPEP]
   , "ICP Threats"                    : V[ONE*ICPThreat]
   , "Partijen"                       : V[ONE*OrganizationalUnit]
   ]
-----------------------------------------------------------
INTERFACE "[COI]" FOR Developer: I[CommunityOfInterest]
BOX[ "Community of Interest" : I 
   , "Goal"                  : coiGoal
   , "Parties"               : coiParty
   , "Benodigde koppelvlakken "    : coiDesiredICP
   ]
-----------------------------------------------------------
INTERFACE "[OrganizationalUnit]" FOR Developer: I[OrganizationalUnit]
BOX[ "ouName" : ouName
   , "ouDescription" : ouDescription
   , "ouCommander" : ouCommander
   ]
-----------------------------------------------------------
INTERFACE "[InformationDomain]" FOR Developer: I[InformationDomain]
BOX[ "Informatie Domein" : I
   , "operationalResponsible" : operationalResponsible
   , "secPolicy" : secPolicy
   , "classificationLevel" : classificationLevel
   ]
-----------------------------------------------------------
INTERFACE "[ICP]" FOR Developer: I[InterconnectionPane]
BOX[ "InterconnectionPane" : I
   , "icpName" : icpName
   , "icpSrc" : icpSrc
   , "icpDst" : icpDst
   , "icpTrustlevel" : icpTrustlevel
   , "icpImplementationDecision" : icpImplementationDecision
   , "icpIsOperational" : icpIsOperational
   ]
-----------------------------------------------------------
INTERFACE "[SPEP]" FOR Developer: I[SPEP]
BOX[ "SPEP"     : I
   , "spepICP" : spepICP
   , "spepType" : spepType
   ]
-----------------------------------------------------------
INTERFACE "[Threat]" (icpThreatICP,icpThreatThreat,icpThreatSignificance,icpThreatSelectedFSRs,icpThreatRestRisico) FOR Developer: I[ICPThreat]
BOX[ "Threat" : I
   BOX[ "icpThreatICP" : icpThreatICP
      , "icpThreatThreat" : icpThreatThreat
      ]
   , "Threat significance (0-4)" : icpThreatSignificance
   , "Geslecteerde maatregel(en)" : icpThreatSelectedFSRs
   , "Restrisico" : icpThreatRestRisico
   ]
-----------------------------------------------------------
INTERFACE "[Threat Mitigation]" (icpThreatSignificance,icpThreatSelectedFSRs,icpThreatRestRisico) FOR Developer: I[ICPThreat]
BOX[ "Threat" : I
   BOX[ "icpThreatICP" : icpThreatICP
      , "icpThreatThreat" : icpThreatThreat
      ]
   , "Threat significance (0-4)" : icpThreatSignificance
   , "Geselecteerde maatregel(en)" : icpThreatSelectedFSRs
   , "Restrisico" : icpThreatRestRisico
-- , "Werklijst"  : <lijst van gesignaleerd werk voor dit type Concept>
   , "Werklijst" : I
   BOX[ "Threat significantie" : (I /\ -(icpThreatSignificance;icpThreatSignificance~));V[ICPThreat*ToDo];'moet worden vastgesteld'[ToDo]
   ]  ]
-----------------------------------------------------------
PROCESS Development
RULE "Do the development": I[YesNo] = I[YesNo]
ROLE "Developer" MAINTAINS "Do the development"
ENDPROCESS
-----------------------------------------------------------
ENDCONTEXT
