{- Population using concept 'Threat'
POPULATION threatID          CONTAINS [ ("Threat_0", "ThreatID") ]
POPULATION threatDescription CONTAINS [ ("Threat_0", "ThreatText") ]
POPULATION threatCategory    CONTAINS [ ("Threat_0", "ThreatCategory") ]
-}

POPULATION threatID          CONTAINS [ ( "Threat_001", "1.A") ]
POPULATION threatDescription CONTAINS [ ( "Threat_001", "Denial of sending information. The sender denies the information is actually sent (released) to the recipient.") ]
