POPULATION threatID          CONTAINS [ ( "Threat_002", "3.B") ]
POPULATION threatDescription CONTAINS [ ( "Threat_002", "Modification of information in transit. The information that is being processed for release or acceptance is modified during the transmission from one SPEP to another or to and from the own and other domain.") ]
POPULATION threatCategory    CONTAINS [ ( "Threat_002", "I") ]

POPULATION threatID          CONTAINS [ ( "Threat_003", "4.B") ]
POPULATION threatDescription CONTAINS [ ( "Threat_003", "Unintentional leakage of information conform the functionality by a user. The user releases information to another domain by mistake. E.g. the user presumes the information is a lower classification than it actually is, or sends the wrong document.") ]
POPULATION threatCategory    CONTAINS [ ( "Threat_003", "A") ]
