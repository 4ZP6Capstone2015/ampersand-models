CONTEXT FactuurTicket2 IN ENGLISH LATEX
--! LET OP: Dit bestand moet je zien in samenhang met FactuurTicket1.adl !--

factuurRegel :: Factuur * Regel
regelText :: Regel * TEXT [UNI]
factuurOK :: Factuur * Factuur [PROP]

RULE "Factuur controle": factuurOK = I[Factuur] /\ (-factuurRegel!regelText);V
MEANING "Definitie: een factuur is ok DESDA de factuur is leeg, of elk van zijn regels bevat een text."

--[Factuur 1]--
POPULATION factuurRegel CONTAINS [ ("Factuur1", "Regel-1-1") ]
POPULATION regelText    CONTAINS [ ("Regel-1-1", "Eentje") ]
