--POPULATION regelText    CONTAINS [ ("Regel-2-1", "Tweede") ]
--POPULATION factuurOK    CONTAINS [ ("Factuur1", "Factuur1") ]

--[Factuur 2]--
POPULATION factuurRegel CONTAINS [ ("Factuur2", "Regel-1-2") ]
POPULATION regelText    CONTAINS [ ("Regel-1-2", "Eentje") ]
--POPULATION factuurOK    CONTAINS [ ("Factuur2", "Factuur2") ]

ENDCONTEXT