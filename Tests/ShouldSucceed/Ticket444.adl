CONTEXT TicketXXX IN DUTCH
--------------------------------------------------------
PROCESS "TicketXXX"

bo :: B * O [UNI,TOT]
r  :: B * D [UNI]
s  :: O * D [UNI]

p  :: D * D [PROP]

RULE "Inv1": p = I /\ (r~;r \/ s~;s)
MEANING "p = I /\\\\ (r~;r \\\\/ s~;s)"
-- bfDashboardNH~;bfDashboardNH \/ ouDashboardNH~;ouDashboardNH |- nhIsDashboard

ROLE ExecEngine MAINTAINS "insP"
RULE "insP": I[D] /\ (r~;r \/ s~;s) |- p[D]
VIOLATION (TXT "{EX} InsPair;p;D;", SRC I, TXT ";D;", TGT I)

ROLE ExecEngine MAINTAINS "delP"
RULE "delP": p[D] |- r~;r \/ s~;s
VIOLATION (TXT "{EX} DelPair;p;D;", SRC I, TXT ";D;", TGT I)

POPULATION bo CONTAINS [ ("HRM", "VB:SA1") ]
POPULATION bo CONTAINS [ ("Pa" , "VB:SA1") ]
POPULATION bo CONTAINS [ ("Pch", "VB:SA1") ]
POPULATION bo CONTAINS [ ("Po" , "VB:SA2") ]
POPULATION bo CONTAINS [ ("Pv" , "VB:SA1") ]

ENDPROCESS

INTERFACE "TEST" (bo,r,s,p) : I[ONE]
BOX[ "B" : V[ONE*B]
   BOX[ "B's" : I
      , "bo" : bo
      , "r" : r
      ]
   , "O" : V[ONE*O]
   BOX[ "O's" : I
      , "bo~" : bo~
      , "s" : s
      ]
   , "D" : V[ONE*D]
   BOX[ "D's" : I
      , "s~" : s~
      , "p" : p
      ]
   ]
--------------------------------------------------------
ENDCONTEXT