CONTEXT Issue88 IN DUTCH

     sPerson :: SESSION * Person [UNI]
     sManagesX :: SESSION * X
     
     CLASSIFY Y ISA X
     
     personManagesX :: Person * X =[ ("John", "Johns X") ]
     personManagesY :: Person * Y =[ ("Pete", "Petes Y"); ("John", "Johns Y") ] 
     
     RULE "sManagement" : sManagesX = sPerson;personManagesX
     VIOLATION (TXT "Sessie=", SRC I, TXT "; sPerson=", SRC sPerson, TXT "; sMgsX=", TGT I)
     
     POPULATION sPerson CONTAINS [ ("S1", "John") ]
     POPULATION sManagesX CONTAINS [ ("S1", "Johns X"); ("S1", "Johns Y") ]
     
     POPULATION sPerson CONTAINS [ ("S2", "Pete") ]
     POPULATION sManagesX CONTAINS [ ("S2", "Petes Y") ]

{+
PROCESS Issue88

ROLE ExecEngine MAINTAINS "Add to sManagesX"
RULE "Add to sManagesX" : sPerson;personManagesX |- sManagesX
VIOLATION (TXT "{EX} InsPair;sManagesX;SESSION;", SRC I, TXT ";X;", TGT I)

ROLE ExecEngine MAINTAINS "Del from sManagesX"
RULE "Del from sManagesX" : sManagesX |- sPerson;personManagesX
VIOLATION (TXT "{EX} DelPair;sManagesX;SESSION;", SRC I, TXT ";X;", TGT I)

ENDPROCESS

INTERFACE Bug (sPerson, personManagesX, personManagesY): '_SESSION'[SESSION]
ROWS[ "sPerson" : sPerson
    , "Things to manage" : sPerson
    COLS[ "X's" : personManagesX
        , "   " : V;'|'[Message]
        , "Y's" : personManagesY
        ]
    , "sManagesX" : sManagesX
    ]
-}
ENDCONTEXT