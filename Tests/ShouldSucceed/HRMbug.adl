CONTEXT HRMbug

r:: A*A [PROP] =[ ("a1", "a1"); ("a3", "a3") ]
s:: A*B        =[ ("a1", "b1"); ("a3", "b3"); ("a3", "b2"); ("a1", "b4") ]

PROCESS Automated

ROLE ExecEngine MAINTAINS insr
RULE insr: I /\ s;s~ |- r
VIOLATION (TXT "{EX} InsPair;r;A;", SRC I, TXT ";A;", TGT I)

ROLE ExecEngine MAINTAINS delr
RULE delr: r |- I /\ s;s~
VIOLATION (TXT "{EX} DelPair;r;A;", SRC I, TXT ";A;", TGT I)

ENDPROCESS

INTERFACE Overview (r,s): I[ONE]
BOX[ A : V[ONE*A]
   BOX[ A : I
      , r : r
      , s : s
      ]
   , B : V[ONE*B]
   BOX[ B : I
      , sflip : s~
      ]
   ]

ENDCONTEXT