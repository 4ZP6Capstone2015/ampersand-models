  SERVICE Letter : I[Document]
   = [ case : caseFile            -- Case
     , type : documentType
     , from : from
     , to : to
     , mark : propertyOf
     , "sent at" : sent
     , "received at" : received
     ]

  SERVICE Document : I[Document]
   = [ type : documentType                            -- DocumentType
     , case : caseFile            -- Casee
        = [ "caretaker of case file" : caretaker,
            "area of law" : areaOfLaw,
            "type of case" : caseType  ]
     ]

  SERVICE Documenten : I[ONE]
   = [ Documenten : V[ONE*Document]
        = [ nr : I[Document]
          , type : documentType
          , "caretaker of case file" : caseFile;caretaker
          , "area of law" : caseFile;areaOfLaw
          , "type of case" : caseFile;caseType
          ]
     ]

  SERVICE DocumentType : I[DocumentType]
   = [ "type Document" : documentType~
        = [ nr : I[Document]
          ]
     ]


  SERVICE Organ : I[Organ]
   = [ "current acts" : organ~;act -- (act~;organ)~
        = [ act : I[Act]
          , prio : use_case~;prio
          , usecase : use_case~
          , rol : may
          ]
     , "actions" : as~
        = [ actie : I[Action]
          , subject : subject
          , type : actionType
          ]
     , "case files" : caretaker~
        = [ case : I[Case]
          , "area of law" : areaOfLaw
          , "type of case" : caseType
          ]
     ]


