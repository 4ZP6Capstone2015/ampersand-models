SERVICE CoreDataUC001 : I[Case] -- UC001 Registreren basisgegevens procedure
{- Het tomaken from een nieuwe procedure middels het registreren from de basisgegevens from een centraal binnengekomen beroepschrift of verzoekschrift om voorlopige voorziening en het automatisch (geen samenhangende procedure togegeven) of handmatig (wel samenhangende procedure togegeven) voorlopig/definitief toewijzen from de nieuwe procedure to een behandelend court.
-}
   = [ plaintiff : plaintiff~;by~/\for~
        = [ party : by
          , representative : authorizedRepresentative~
          ]
     , defendant : defendant~;by~/\for~
        = [ party : by
          , representative : authorizedRepresentative~
          ]
     , "joined party" : joinedInterestedParty~;by~/\for~
        = [ party : by
          , representative : authorizedRepresentative~
          ]
     , "area of law" : areaOfLaw
     , "type of case" : caseType
     , authorized : authorized
     , "caretaker of case file" : caretaker             -- Organ
     , "case file" : caseFile~                     -- [Document]
        = [ Document : I[Document]
          , type : documentType
          ]
     , cluster : cluster
        = [ name  : I[Cluster]
          , base : base ]
     , "authorization documents" :  for~
        = [ document : I[Authorization]
          , represented : by
          , representative : authorizedRepresentative~
          ]
     ]

SERVICE LegalCase : I[Case]
   = [ plaintiff : plaintiff~          -- Party
     , "representative of plaintiff" : (plaintiff~;by~/\for~);authorizedRepresentative~
     , defendant : defendant~          -- Party
     , "representative of defendant" : (defendant~;by~/\for~);authorizedRepresentative~
     , "joined party" : joinedInterestedParty~          -- Party
     , "representative of joined party" : (joinedInterestedParty~;by~/\for~);authorizedRepresentative~
     , "area of law" :  areaOfLaw
     , "type of case" : caseType
     , process : case~
        = [ nr : I[Process]
          , session : session
          , panel : session;panel
          , scheduled : session;scheduled
          , judge : session;judge
          , clerk : session;clerk
          ]
     , authorized : authorized
     , "caretaker of case file" : caretaker           -- Organ
     , "case file" : caseFile~                     -- [Document]
        = [ Document : I[Document]
          , type : documentType
          ]
     , "authorization documents" :  for~
        = [ authorization : I[Authorization]
          , party : by
          , representative : authorizedRepresentative~
          ]
     ]

SERVICE Cluster : I[Cluster]
   = [ name : name
     , base : base
     , "case" : cluster~
         = [ "caretaker of case file": caretaker
           , "area of law" :  areaOfLaw
           , "type of case" : caseType]]

SERVICE Clusters : I[ONE]
   = [ Clusters : V[ONE*Cluster]
         = [ nr : I[Cluster]
           , name : name
           , cases : cluster~
               = [ "caretaker of case file": caretaker
                 , "area of law" :  areaOfLaw
                 , "type of case" : caseType
                 ]
           ]
     ]

  SERVICE Cases : I[ONE]
   = [ Cases : V[ONE*Case]
        = [ nr : I[Case]
          , session : case~;session
          , "area of law" :  areaOfLaw
          , "type of case" : caseType
          , "caretaker voor dossier" : caretaker
          , court : case~;session;location
          , clusters : cluster
             = [ cases : cluster~ ]
          ]
     ]

SERVICE newCase : I[Case]
   = [ "caretaker voor dossier" : caretaker
     , "area of law" :  areaOfLaw
     , "type of case" : caseType]

  SERVICE Session : I[Session]
   = [ court : location
     , panel : panel
     , city : city
     , judge : judge
     , clerk : clerk
     , scheduled : scheduled
     , "date of occurence" : occured
     , cases : (case~;session)~
        = [ nr : I[Case]
          , plaintiff : plaintiff~
          , defendant : defendant~
          ]
     ]

