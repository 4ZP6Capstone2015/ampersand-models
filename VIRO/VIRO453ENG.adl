  SERVICE Sector : I[Sector]
   = [ "sector Panel" : sector~
        = [ Panel : I[Panel]
          , court : court
          ]
     ]

  SERVICE Court : I[Court]
   = [ Sessions : location~
        = [ nr : I[Session]
          , panel : panel
          , judge : judge
          , clerk : clerk
          , scheduled : scheduled
          , schedule : session~
             = [ nr : I[Process]
               , case : case
               , "type of case" : case;caseType
               ]
          ]
     , panels : court~
     , members : clerk \/ court~;members~
     , cases : authorized~
     , "main city" : mainCity
     , "local cities" : localCities~
     , "o schedule" : authorized~ /\ -(location~;session~;case) -- de rechtbank heeft kennis genomen van de case en de case staat nog niet op de rol...
             = [ casenr   : I[Case]
               , defendant : defendant~
               , plaintiff    : plaintiff~
               , "joined party" : joinedInterestedParty~
               ]
     ]

  SERVICE Article : I[Article]
