   "Dagvaardingen zonder zaaknummer" : V[ONE*Document];type;'Dagvaarding';type~ /\ -(V[ONE*Document];type;'Dagvaarding';type~;zaaksdocument;zaakid;zaakid~;zaaksdocument~)
]


INTERFACE Zaken FOR "Poortraadsheer" 
