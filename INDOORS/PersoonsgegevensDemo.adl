CONTEXT PersoonsgegevensDemo

--INCLUDE "INDOORS.adl"
INCLUDE "Persoonsgegevens.adl"

INTERFACE NatuurlijkPersoon (naam, gbaBSN, gbaTitelPredikaat~, opleidingstitel, gbaGeboortedatum, gbaGeboorteland, gbaGeboorteplaats, woonplaats, gbaGeslacht, gba) 
                          : I[NatuurlijkPersoon]
BOX [ Naam                : naam
    , BurgerServiceNummer : gbaBSN
    , "Adelijke titel"    : gbaTitelPredikaat~~
	, Opleidingstitel     : opleidingstitel~
    , Geboortedatum       : gbaGeboortedatum
    , "Land v. herkomst"  : gbaGeboorteland
    , Geboorteplaats      : gbaGeboorteplaats
    , Woonplaats          : woonplaats
    , Geslacht            : gbaGeslacht
    , "GBA-nummer"        : gba~
    ]
	
INTERFACE "Verifieer persoonsgegevens" (gbaBSN) : I[NatuurlijkPersoon]
BOX [ BurgerServiceNummer : gbaBSN
      BOX [ Naam                : gbaBSN~;naam
          , "Adelijke titel"    : gbaBSN~;gbaTitelPredikaat~~
	      , Opleidingstitel     : gbaBSN~;opleidingstitel~
          , Geboortedatum       : gbaBSN~;gbaGeboortedatum
          , "Land v. herkomst"  : gbaBSN~;gbaGeboorteland
          , Geboorteplaats      : gbaBSN~;gbaGeboorteplaats
          , Woonplaats          : gbaBSN~;woonplaats
          , Geslacht            : gbaBSN~;gbaGeslacht
          , "GBA-nummer"        : gbaBSN~;gba~	      
	      ]
    ]


ENDCONTEXT
