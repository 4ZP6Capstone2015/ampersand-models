   "Binnengekomen klaagschriften"  : V[ONE*Zaakid];zaaksdocument~;type;'Klaagschrift';type~;(-zaaksdocument;zaaksdocument~;type;'Ontvangstbevestiging';type~)
,  "Nog naar AG te versturen"      : V[ONE*Zaakid];zaaksdocument~;type;'Klaagschrift';type~;(zaaksdocument;zaaksdocument~;type;'Ontvangstbevestiging';type~) --/\ -(V[ONE*Zaakid];zaaksdocument~;type;'Verzoek schriftelijk verslag';type~)
,  "Klaagschriften op zitting"     : V[ONE*Zaakid];zaaksdocument~;type;'Klaagschrift';type~;zaaksdocument /\ V[ONE*Zaakid];behandeld;behandeld~
,  "Zitting afronden"              : V[ONE*Zaakid];zaaksdocument~;type;'Klaagschrift';type~;zaaksdocument /\ V[ONE*Zaakid];zaaksdocument~;type;'Schriftelijk verslag';type~;zaaksdocument /\ -(V[ONE*Zaakid];zaaksdocument~;type;'Beschikking';type~;zaaksdocument)
,  "Administratief afdoen"         : V[ONE*Zaakid];zaaksdocument~;type;'Klaagschrift';type~;zaaksdocument /\ V[ONE*Zaakid];zaaksdocument~;type;'Schriftelijk verslag';type~;zaaksdocument /\ V[ONE*Zaakid];zaaksdocument~;type;'Beschikking';type~;zaaksdocument
]

